<?xml version="1.0" encoding="utf-8"?>

<!--

    custom_gauge data is comming from attrs.xml in values!!!

-->

<com.threedevs.aj.HwInfoReceiver.CustomGauge
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:custom_gauge="http://schemas.android.com/apk/res-auto"

    android:layout_width="120dp"
    android:layout_height="120dp"

    android:paddingBottom="20dp"
    android:paddingLeft="20dp"
    android:paddingRight="20dp"
    android:paddingTop="20dp"


    custom_gauge:pointStartColor="@color/gauge_start_color"
    custom_gauge:pointEndColor="@color/gauge_end_alternative_color"


    custom_gauge:strokeCap="ROUND"
    custom_gauge:strokeColor="@color/gauge_background_color"
    custom_gauge:strokeWidth="15dp"
    custom_gauge:startValue="0"
    custom_gauge:endValue="100"
    custom_gauge:sweepAngel="270"
    custom_gauge:titleSize="14dp"
    >


</com.threedevs.aj.HwInfoReceiver.CustomGauge>