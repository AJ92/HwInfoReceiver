<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CustomGauge">
        <attr name="pointSize" format="integer" />
        <attr name="pointStartColor" format="color" />
        <attr name="pointEndColor" format="color" />
        <attr name="strokeColor" format="color" />
        <attr name="strokeWidth" format="dimension" />
        <attr name="strokeCap" format="string" />
        <attr name="startAngel" format="integer" />
        <attr name="sweepAngel" format="integer" />
        <attr name="startValue" format="integer" />
        <attr name="endValue" format="integer" />
        <attr name="titleValue" format="string" />
        <attr name="titleSize"  format="dimension"/>
    </declare-styleable>

    <attr name="main_menu_add_server" format="reference" />
    <attr name="main_menu_help" format="reference" />
    <attr name="main_menu_settings" format="reference" />

    <attr name="server_menu_add_sensor" format="reference" />
    <attr name="server_menu_help" format="reference" />
    <attr name="server_menu_settings" format="reference" />
</resources>
